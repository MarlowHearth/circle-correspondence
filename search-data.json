---
layout: null
---
[
  {% assign filtered_pages = site.pages | where_exp: "page", "page.title" %}
  {% for page in filtered_pages %}
    {% unless page.url contains 'search-data.json' or page.url contains 'assets/' %}
    {
      "title": {{ page.title | jsonify }},
      "url": {{ page.url | replace: '.html', '' | jsonify }},
      "content": {{ page.content | strip_html | strip_newlines | truncatewords: 200 | jsonify }},
      "excerpt": {{ page.content | strip_html | truncatewords: 30 | jsonify }}
    }{% unless forloop.last %},{% endunless %}
    {% endunless %}
  {% endfor %}
]
